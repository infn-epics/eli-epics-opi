    <pv>
      <display_name>LLRF01 CPU</display_name>
      <visible>true</visible>
      <name>LEL-RF-LLRF01:ioc:cpu</name>
      <axis>1</axis>
      <color>
        <red>255</red>
        <green>0</green>
